        <div class="row ">
            <% books.forEach(books=> { %>

                <div class="col-sm-4 col-lg-2  grid-margin stretch-card ">
                    <div class="card ">
                        <img class="book-img section" src="bookImages/<%= books.image[0].mainImage %> " alt="">
                        <div class="card-body text-center">
                            <p class="book-title"> </p>
                            <div>
                                <p class="">
                                    <%= books.bookTitle%>
                                </p>
                                <h4 class="text-success"><i class="mdi mdi-cash-multiple"></i> 100</h4>
                                <h5 class="">Delivery charge</h5>
                            </div>
                            <div class="row ">
                                <button onclick="addToCart(`<%=books._id %>`)"
                                    class="btn btn-outline-info col-4 text-center">
                                    <i class="mdi mdi-cart-plus"></i>
                                </button>
                                <button onclick="addToWishList(`<%=books._id %>`)"
                                    class=" btn btn-outline-danger col-4 text-center">
                                    <i class="mdi mdi-heart-outline"></i>
                                </button>
                                <button onclick="addToGift(`<%=books._id %>`)"
                                    class=" btn btn-outline-warning col-4 text-center">
                                    <i class="mdi mdi-gift"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <% }) %>
        </div>



